{% extends 'base.html.twig' %}
{% block title %} Character{% endblock %}
    {% block body %}
    <div class="row">
        <div class="col col-lg-12">
            <table class="table espacio">
                <tbody>
                    <tr>
                        <th>Id</th>
                        <td>{{ character.id }}</td>
                    </tr>
                    <tr>
                        <th>Name</th>
                        <td>{{ character.name }}</td>
                    </tr>
                    <tr>
                        <th>Gender</th>
                        <td>{{ character.gender }}</td>
                    </tr>
                </tbody>
            </table>
        </div>
    </div>
    <p><a href="{{ path('character') }}">Character</a></p>
{% endblock %}
